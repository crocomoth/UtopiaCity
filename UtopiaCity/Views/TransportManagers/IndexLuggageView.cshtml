@model IEnumerable<UtopiaCity.Models.Airport.AirportWarehouse>

@{
    ViewData["Title"] = "IndexLuggageView";
}

<h1>Main Airport Warehouse</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.LuggageType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LuggageWeight)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HostName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.LuggageType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LuggageWeight)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HostName)
            </td>
            <td>
                <a asp-action="EditLuggage" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="DetailsLuggage" asp-route-id="@item.Id">Details</a> |
                <a asp-action="DeleteLuggage" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
<p>
    <a asp-action="Index">Back to List</a>
</p>
